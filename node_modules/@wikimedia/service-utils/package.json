{
  "name": "@wikimedia/service-utils",
  "version": "1.4.2",
  "description": "",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup-node src/index.ts --format cjs,esm --cjsInterop --splitting",
    "watch": "npm run build -- --watch src",
    "prepublishOnly": "npm run build",
    "test": "vitest --poolOptions.threads.singleThread true",
    "lint": "eslint ./",
    "lint:fix": "npm run lint -- --fix ./"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=20"
  },
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "tsup": {
    "dts": {
      "compilerOptions": {
        "module": "nodenext",
        "moduleResolution": "nodenext",
        "allowImportingTsExtensions": true
      }
    },
    "sourcemap": true,
    "clean": true
  },
  "dependencies": {
    "@elastic/ecs-winston-format": "^1.5.3",
    "@opentelemetry/api": "^1.9.0",
    "c12": "^1.10.0",
    "prom-client": "^15.1.0",
    "uuid": "^11.0.3",
    "winston": "^3.13.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@opentelemetry/core": "^1.27.0",
    "@stylistic/eslint-plugin": "^2.6.4",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/uuid": "^10.0.0",
    "@types/supertest": "^6.0.2",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^8.57.1",
    "eslint-config-wikimedia": "^0.28.2",
    "express": "^4.19.2",
    "globals": "^15.9.0",
    "supertest": "^7.0.0",
    "tsup": "^8.0.2",
    "typescript": "^5.6.3",
    "vitest": "^1.4.0"
  }
}
